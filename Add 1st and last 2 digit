import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        if(a<999){
            System.out.println("Invalid");
        }
        else{
            int last = a%100;
            int first = a;
            while(first>99) {
                first = first/10;
            }
            System.out.println(last + first);
        }
    }
}
