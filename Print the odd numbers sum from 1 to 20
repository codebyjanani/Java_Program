public class Main {
    public static void main(String[] args) {
        int[] numbers = new int[20];
        for (int i = 0; i < 20; i++) {
            numbers[i] = i + 1; 
        }
        int sum = 0; 
        for (int num : numbers) {
            if (num % 2 != 0) { 
                sum += num; 
            }
        }
        System.out.println("The sum of odd numbers from 1 to 20 is: " + sum);
    }
}
